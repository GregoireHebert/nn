{
    "name": "numphp/numphp",
    "type": "library",
    "description": "Mathematical PHP library for scientific computing",
    "keywords": ["numeric", "matrix", "linalg"],
    "homepage": "http://numphp.org/",
    "license": "MIT",
    "authors": [
        {
            "name": "Gordon Lesti",
            "email": "info@gordonlesti.com",
            "homepage": "http://gordonlesti.com/",
            "role": "developer"
        }
    ],
    "require": {
        "php": ">=5.4"
    },
    "require-dev": {
        "phpunit/phpunit": "*",
        "phpmd/phpmd" : "*",
        "squizlabs/php_codesniffer": "*",
        "sebastian/phpcpd": "*",
        "satooshi/php-coveralls": "v1.0.1",
        "symfony/console": "~2.2"
    },
    "autoload": {
        "psr-0": {
            "NumPHP\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-0": {
            "NumPHPTest\\": "tests/",
            "NumPHPExamples\\": "examples/"
        }
    }
}
